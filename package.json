{
  "name": "stream-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:prod": "ng serve -c production",
    "start:gtvprod": "ng serve -c gtvproduction",
    "start:dev": "ng serve -c development --host 0.0.0.0",
    "build": "NODE_OPTIONS=--openssl-legacy-provider node --max_old_space_size=3000 ./node_modules/@angular/cli/bin/ng build --configuration production && npm run postbuild",
    "build:prod": "node --max_old_space_size=3000 ./node_modules/@angular/cli/bin/ng build --configuration production && npm run postbuild",
    "build:gtvprod": "node --max_old_space_size=3000 ./node_modules/@angular/cli/bin/ng build --configuration gtvproduction && npm run postbuild",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node web3-patch.js",
    "postbuild": "node post-build.js",
    "prebuild": "",
    "prestart": "",
    "init:dfx": "export NODE_ENV=production && npm run build:prod && npm run restart:clean:dfx && npm run generate:dfx",
    "deploy:dfx": "export NODE_ENV=production && npm run build:prod && dfx deploy --network ic --no-wallet",
    "restart:dfx": "dfx stop && dfx start --background",
    "restart:clean:dfx": "dfx stop && dfx start --background --clean",
    "generate:dfx": "dfx canister create --all && dfx generate && npm run resolve:dfx",
    "resolve:dfx": "find src/declarations -type f -name '**.ts' -delete"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.4.0",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "~13.4.0",
